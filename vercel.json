{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api/app.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}